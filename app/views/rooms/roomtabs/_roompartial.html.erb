<table class="table table-hover">
<th>S.no</th>
<% if current_user %>
	<th>Booked by</th>
	<th>Title</th>
	<th>Description</th>
<%end %>
<th>Start Time</th> 
<th>End Time</th>
	
	<% @croom[tab_id].each_with_index do |room, i| %>
  <tr>
  	<td><%= i+1 %></td>

    <% if current_user %>
    <td>
    	<%= current_user.name%>
    </td>
    <td>
    	<%= room.title%>
    </td>
     <td>
    	<%= room.description%>
    </td>
    <% end %>
   
    <td><% datetime1 = room.start_time %>
    	<%= datetime1.to_formatted_s(:short) %>
    </td>
    <td><% datetime2 = room.end_time %>
    	<%= datetime2.to_formatted_s(:short)%>
    </td>
  </tr>
  <% end %>
</table>